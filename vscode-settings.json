{
    "editor.tabSize": 2,
    "editor.fontFamily": "Dank Mono",
    "editor.fontSize": 16,
    "editor.fontLigatures": true,
    "editor.fontWeight": "600",
    "editor.wordWrap": "on",
    "editor.rulers": [
        100
    ],
    "zenMode.hideLineNumbers": false,
    "ruby.intellisense": true,
    // Basic settings: turn linter(s) on
    "ruby.lint": {
        "reek": true,
        "rubocop": true,
        "ruby": true, //Runs ruby -wc
        "fasterer": true,
        "debride": true,
        "ruby-lint": true,
        "ruby": {
            "unicode": true //Runs ruby -wc -Ku
        },
        "rubocop": {
            "only": ["SpaceInsideBlockBraces", "LeadingCommentSpace"],
            "lint": true,
            "rails": true
        },
        "reek": true
    },

    // Time (ms) to wait after keypress before running enabled linters. Ensures
    // linters are only run when typing has finished and not for every keypress adfa adsdfa dadfa af 
    "ruby.lintDebounceTime": 500,
    "explorer.confirmDelete": false,
    // "vim.useSystemClipboard": true,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
        "before": ["j", "j"],
        "after": ["<Esc>"]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
        "before": ["<leader>", ","],
        "after": ["d", "d"]
        },
        {
        "before": ["<C-n>"],
        "commands": [":nohl"]
        }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false
    },
    "workbench.editor.enablePreview": false,
    "terminal.integrated.rendererType": "dom",
    "files.exclude": {
        "USE_GITIGNORE": true,
    },
    "cSpell.language": "en",
    "breadcrumbs.enabled": true,
    // "terminal.integrated.fontFamily": "'Source Code Pro for Powerline', 'Hack Nerd Font'"
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "terminal.integrated.rendererType": "canvas",
    "terminal.integrated.fontSize": 15,
    "terminal.integrated.fontWeight": "100",
    "terminal.integrated.scrollback": 100000000,
    "terminal.integrated.profiles.osx": {
        "bash": {
            "path": "bash",
            "args": [
                "-l"
            ],
            "icon": "terminal-bash"
        },
        "zsh": {
            "path": "/bin/zsh",
            "args": [
                "-l"
            ],
            "icon": "star",
            "env": {}
        },
        "fish": {
            "path": "fish",
            "args": [
                "-l"
            ]
        },
        "tmux": {
            "path": "tmux",
            "icon": "terminal-tmux"
        },
        "pwsh": {
            "path": "pwsh",
            "icon": "terminal-powershell"
        },
        "csh": {
            "path": "/bin/csh"
        }
    },
    "terminal.integrated.defaultProfile.osx": "zsh",
    "atlascode.jira.workingSite": {
        "baseUrlSuffix": "atlassian.net"
    },
    "yaml.schemas": {
        "file:///Users/waqas/.vscode-insiders/extensions/atlassian.atlascode-2.10.12/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
