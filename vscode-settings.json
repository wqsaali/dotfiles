        {
            "id": "2b60775e-cee6-4c87-ba5d-2edcc768cfdb",
            "enabled": true,
            "name": "My sendoso Issues",
            "query": "assignee = currentUser() AND status not in (Done, Accepted, \"Pushed to Prod\", \"Pushed Live\", \"Not Selected for Work\", Duplicate) ORDER BY lastViewed DESC",
