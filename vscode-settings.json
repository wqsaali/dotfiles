    "atlascode.jira.workingProject": {
        "id": "10000",
        "name": "Platform Board",
        "key": "PB"
    },
    "todohighlight.isEnable": true,
    "todo-tree.highlights.customHighlight": {
        "TODO": {
            "icon": "check",
            "type": "line"
        },
        "[ ]": {
            "icon": "check",
            "type": "line"
        },
        "FIXME": {
            "iconColour": "red"
        },
        "BUG": {
            "iconColour": "red"
        },
        "DONE":{
            "icon": "check",
            "type": "line",
            "iconColour": "green",
            "background": "#00ff0080"
        },
        "[x]":{
            "icon": "check",
            "type": "line",
            "iconColour": "green",
            "background": "#00ff0080"
        }
    },
    "todo-tree.highlights.defaultHighlight": {
        "icon": "alert",
        "type": "text",
        "iconColour": "yellow"
    },
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "todo-tree.general.tags": [
            "BUG",
            "HACK",
            "FIXME",
            "TODO",
            "NOTICE",
            "NOTE",
            "DONE",
            "[ ]",
            "[x]"
        ],
